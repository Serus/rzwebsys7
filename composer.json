{
	"name": "webadmin87/rzwebsys7",
	"description": "RzWebSys7 - CMS powered by Yii Framework 2",
	"keywords": ["rzwebsys7", "yii2", "cms"],
	"homepage": "https://github.com/webadmin87/rzwebsys7",
	"type": "project",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/webadmin87/rzwebsys7/issues",
		"source": "https://github.com/webadmin87/rzwebsys7"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.0-beta",
		"yiisoft/yii2-bootstrap": "2.0.0-beta",
		"yiisoft/yii2-swiftmailer": "2.0.0-beta",
        "yiisoft/yii2-imagine": "2.0.0-beta",
        "yiisoft/yii2-jui": "2.0.0-beta",
        "mihaildev/yii2-ckeditor": "*",
        "mihaildev/yii2-elfinder": "*",
        "creocoder/yii2-nested-set-behavior": "*",
		"marciocamello/yii2-x-editable": "dev-master",
		"newerton/yii2-fancybox": "dev-master",
		"2amigos/yii2-multi-select-widget": "*",
		"yiisoft/yii2-apidoc": "2.0.0-beta",
		"yiisoft/yii2-sphinx": "2.0.0-beta"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "2.0.0-beta",
		"yiisoft/yii2-debug": "2.0.0-beta",
		"yiisoft/yii2-gii": "2.0.0-beta"
	},
	"suggest": {
		"codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
		"codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
		"codeception/verify": "BDD Assertions for PHPUnit and Codeception",
		"yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [

			"console/runtime",
			"console/migrations",

			"app/runtime",
			"app/web/assets"
		]
	},
    "autoload": {
        "psr-0": {
            "common\\": "",
            "console\\": "",
            "app\\": ""
        }
    }
}
